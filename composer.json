{
    "name": "soluzione-software/laravel-iubenda",
    "description": "Laravel package for iubenda.com integration.",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Martini",
            "email": "y.martini@soluzionesoftware.com",
            "homepage": "https://github.com/yuriy-martini",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/translation": "^5.8|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SoluzioneSoftware\\Iubenda\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoluzioneSoftware\\Iubenda\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
